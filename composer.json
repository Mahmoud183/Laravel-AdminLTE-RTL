{
    "name": "acharsoft/laravel-adminlte-rtl",
    "description": "Easy AdminLTE integration with Laravel with rtl support",
	"keywords": ["laravel", "adminlte", "admin", "administrator","rtl"],
    "require": {
        "laravel/framework": "~5.1.12|~5.2.0|~5.3.0|~5.4.0|~5.5.0|~5.6.0|~5.7.0",
        "php": ">=5.5.9"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "mobin poursalami",
            "email": "mobinpou@gmail.com"
        }
    ],
	"autoload": {
		"psr-4": {
		  "acharsoft\\LaravelAdminLte\\": "src/"
		}
	},
	"autoload-dev": {
		"classmap": [
		  "tests/TestCase.php"
		]
	},
	"extra": {
      "laravel": {
          "providers": [
              "acharsoft\\LaravelAdminLte\\ServiceProvider"
          ]
      }
	}
}
